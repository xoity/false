{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && npx medusa db:migrate && mkdir -p public && ln -sfn \"$(pwd)/.medusa/server/public/admin\" public/admin",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "create-admin": "medusa exec ./src/scripts/create-admin.ts",
    "setup-shipping": "medusa exec ./src/scripts/setup-shipping.ts",
    "fix-shipping-prices": "medusa exec ./src/scripts/fix-shipping-prices.ts",
    "add-shipping-prices": "medusa exec ./src/scripts/add-shipping-prices.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "prepare": "husky"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.12.2",
    "@medusajs/cache-redis": "^2.13.1",
    "@medusajs/cli": "^2.12.2",
    "@medusajs/event-bus-redis": "^2.13.1",
    "@medusajs/framework": "^2.12.2",
    "@medusajs/medusa": "^2.12.2",
    "@medusajs/payment-stripe": "^2.12.2",
    "@swc/core": "^1.7.28",
    "connect-redis": "^7.1.1",
    "esbuild": "^0.25.12",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.7.1",
    "stripe": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^7.2.7"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.12.2",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^7.2.7",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
